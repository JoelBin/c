#include <stdio.h>
//求最大公约数
/*
a   b   t       a除以b的余数：a%b,a=b,b=t,如果b=0,就结束，输出最大公约数a
12 18   12
18 12   6
12 6    0
6   0
*/
int main()
{
    int a, b;
    int t;
    scanf_s("%d %d", &a,&b);
        while (b != 0) {
            t = a % b;
            a = b;
            b = t;
            printf("a=%d,b=%d,t=%d\n", a, b, t);
        }
        printf("gcd=%d", a);
        return 0;
}
